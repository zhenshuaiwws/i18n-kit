{
  "name": "i18n-kit",
  "version": "0.2.0",
  "description": "Help you deal with I18N internationalization, extraction from code based on Babel, import and export excel, generate translation JSON file.",
  "author": {
    "name": "zhenshuai",
    "email": "231640921@qq.com"
  },
  "main": "index.js",
  "bin": {
    "i18n-kit": "index.js"
  },
  "repository": "https://github.com/zhenshuaiwws/i18n-kit",
  "scripts": {
    "test": "node index.js code2json --code-folder-path demo/code --json-path demo/lang/zh.json",
    "test-config": "node index.js code2json --code-folder-path demo/code --json-path demo/lang/zh.json --config demo/config.js",
    "test-log": "node index.js code2json --code-folder-path demo/code --json-path demo/lang/zh.json --log",
    "test2": "node index.js code2excel --code-folder-path demo/code --excel-path demo/excel/t.xlsx"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/generator": "^7.14.1",
    "@babel/parser": "^7.13.10",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-transform-typescript": "^7.13.0",
    "@babel/traverse": "^7.14.0",
    "ast-query": "^2.0.0",
    "babel-traverse": "^6.26.0",
    "commander": "^7.1.0",
    "esprima": "^4.0.1",
    "exceljs": "^4.2.0",
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "single-line-log": "^1.1.2",
    "terminal-kit": "^2.0.5"
  },
  "devDependencies": {
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1"
  }
}
